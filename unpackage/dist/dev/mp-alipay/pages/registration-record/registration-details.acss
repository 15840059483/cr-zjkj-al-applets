.payment-success-wrapper {
  padding: .5rem;
}
.payment-success-wrapper .payment-success {
  font-size: .5rem;
  color: #008cfe;
  margin-bottom: .2rem;
}
.payment-success-wrapper .payment-tip {
  font-size: .3rem;
  margin-top: .5rem;
  line-height: .6rem;
}
.payment-ma-wrapper {
  padding: .5rem;
  margin: .5rem 0;
  text-align: center;
}
.payment-ma-wrapper ._img {
  width: 70%;
  height: 1.5rem;
  margin: .1rem 0;
}
.payment-list-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: .3rem .5rem;
  margin-bottom: 1px;
}
.payment-list-item .payment-item-right {
  color: #e1e1e1;
}
.table-title {
  padding: 0 0 .3rem .5rem;
  font-weight: bold;
}
.cell {
  text-align: center;
  color: #c6c6c6;
}
.payment-total-amount {
  padding: .3rem .5rem;
  font-weight: bold;
}
.payment-total-amount ._span {
  color: #c6c6c6;
  font-weight: 400;
}

