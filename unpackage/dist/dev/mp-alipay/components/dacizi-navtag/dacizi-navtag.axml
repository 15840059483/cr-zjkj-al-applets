<view class="nav_tab"><view class="buttom_height"></view><view class="tab_button"><view class="tab_item" data-current="{{index}}" data-event-opts="{{[['tap',[['switchTab',[index]]]]]}}" onTap="__e" a:for="{{tab_button}}" a:for-item="item" a:for-index="index"><image src="{{item_index==index?item.light_icon:item.default_icon}}"></image><view class="{{(item_index==index?'active_item':'')}}">{{item.itemText}}</view></view></view></view>